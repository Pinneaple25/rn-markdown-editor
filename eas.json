{
  "cli": {
    "version": ">= 14.4.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "releaseChannel": "production",
      "distribution": "store",
      "android": {
        "buildType": "apk"
      }
    },
    "production": {
      "releaseChannel": "production",
      "autoIncrement": true,
      "distribution": "store",
      "android": {
        "androidVersionCode": 2,
        "buildType": "app-bundle"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
